generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Recipe {
  id             String           @id @default(uuid())
  name           String
  urlImage       String
  description    String
  category       String
  cookTimer      String
  ingredients    String[]
  instructions   String[]

  @@map("recipes")
}

